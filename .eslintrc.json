{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "eslint:recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": "off",
    "prefer-arrow-callback": "error",
    "no-unused-vars": "off",
    "react/prop-types": "off",
    "quotes": ["warn", "double"],
    "react/no-unescaped-entities": 0
  }
}
